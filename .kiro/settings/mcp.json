{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem@latest",
        "./thai-ride-app"
      ],
      "disabled": false,
      "autoApprove": [
        "read_file",
        "read_multiple_files",
        "list_directory",
        "search_files",
        "get_file_info",
        "read_text_file",
        "write_file",
        "list_allowed_directories"
      ]
    }
  }
}